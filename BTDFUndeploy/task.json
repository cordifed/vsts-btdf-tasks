{
  "id": "0ecf2c9d-c898-4a23-96ec-c8e96ee2a405",
  "name": "BTDFUndeployTask",
  "friendlyName": "BTDF - Undeploy ",
  "description": "Deployment Framework for BizTalk btdfproj project target: Undeploy",
  "author": "Jason Vercellone and Fr√©deric Cordier",
  "helpMarkDown": "",
  "category": "Deploy",
  "visibility": [
    "Release"
  ],
  "demands": [],
  "version": {
    "Major": "0",
    "Minor": "1",
    "Patch": "1"
  },
  "minimumAgentVersion": "1.95.0",
  "instanceNameFormat": "UnDeploy: $(Name)",
  "groups": [
    {
        "name": "options",
