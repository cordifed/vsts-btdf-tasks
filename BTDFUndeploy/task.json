      "helpMarkDown": "BizTalk application name."
    },
    {
      "name": "BTDeployMgmtDB",
      "type": "boolean",
      "label": "Deploy Mgmt DB",
      "defaultValue": "true",
      "helpMarkDown": "Boolean DeployBizTalkMgmtDB MSBuild property setting used when building the BTDF project (*.btdfproj)."
    },
    {
      "name": "ProgramFilesDir",
      "type": "string",
      "label": "Program Files Folder",
      "defaultValue": "",
      "required": false,
      "helpMarkDown": "Program Files Folder.",
      "groupName": "options"
    },
    {
      "name": "ProgramName",
      "type": "string",
      "label": "Product Name",
      "defaultValue": "",
      "required": false,
      "helpMarkDown": "Installed product name, if different from the BizTalk application name.",
      "groupName": "options"
    }
  ],
  "execution": {
    "PowerShell": {
      "target": "$(currentDirectory)\\UnDeploy-BTDFApplication.ps1",
      "argumentFormat": "",
      "workingDirectory": "$(currentDirectory)"
    }
